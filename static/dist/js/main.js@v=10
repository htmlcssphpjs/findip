$(function(){var a=$(".self-ipv4-block .language-bash"),o=$(".self-ipv6-block .language-bash");$.getJSON("https://api.ipify.org?format=jsonp&callback=?",function(e){a.text(a.text().replace("185.103.110.182",e.ip)),Prism.highlightElement($(a)[0]),$(a).closest(".self-ipv4-block").animate({opacity:1},300)}),$.getJSON("https://api64.ipify.org?format=jsonp&callback=?",function(e){o.text(o.text().replace("2a00:1450:400f:80d::200e",e.ip)),Prism.highlightElement($(o)[0]),$(o).closest(".self-ipv6-block").animate({opacity:1},300)}),$(".code-samples-block").find("code").each(function(){Prism.highlightElement(this)})});var activeWaypointAnchor=null,selector=$("header .second-level .selector"),waypoints=$(".waypoint-particle").waypoint({handler:function(){activeWaypointAnchor=$('header .second-level a[data-anchor="'+$(this.element).data("waypoint")+'"]')[0],$(selector).animate({width:$(activeWaypointAnchor).width(),left:activeWaypointAnchor.offsetLeft},300)},offset:"200px"}),waypointBlock=null,offset=0;$("header .second-level a").click(function(){waypointBlock=$('.waypoint-block[data-waypoint="'+$(this).data("anchor")+'"]')[0],offset=$(document).scrollTop()>waypointBlock.offsetTop?120:60,$("html, body").animate({scrollTop:waypointBlock.offsetTop-offset},500)});var didScroll,lastScrollTop=0;function hasScrolled(){var e=$(window).scrollTop();30<e?(lastScrollTop<e?$("header").hasClass("hidden-header")||$("header").addClass("hidden-header"):$("header").hasClass("hidden-header")&&$("header").removeClass("hidden-header"),lastScrollTop=e):$("header").removeClass("hidden-header"),50<e?$("header").addClass("scrolled-header"):$("header").removeClass("scrolled-header"),100<e?$(".up-btn").show(300):$(".up-btn").hide(300)}$(window).scroll(function(e){didScroll=!0}),setInterval(function(){didScroll&&(hasScrolled(),didScroll=!1)},250);
